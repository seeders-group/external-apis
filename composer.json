{
    "name": "seeders-group/external-apis",
    "description": "Laravel package providing external API integrations for SEO, AI, and marketing tools",
    "keywords": [
        "laravel",
        "api",
        "seo",
        "ahrefs",
        "dataforseo",
        "openai",
        "saloon"
    ],
    "homepage": "https://github.com/seeders-group/external-apis",
    "license": "MIT",
    "authors": [
        {
            "name": "Seeders Group",
            "email": "dev@seeders.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "guzzlehttp/guzzle": "^7.8",
        "google-gemini-php/client": "^1.0",
        "google/apiclient": "^2.15.0",
        "illuminate/contracts": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0",
        "openai-php/client": "^0.16",
        "saloonphp/laravel-plugin": "^3.5",
        "saloonphp/saloon": "^3.0",
        "spatie/laravel-data": "^4.0"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Seeders\\ExternalApis\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Seeders\\ExternalApis\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Seeders\\ExternalApis\\ExternalApisServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
